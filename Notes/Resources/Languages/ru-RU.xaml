    <Image x:Key="Italic">
        <Image.Source>
            <BitmapImage UriSource="/Resources/Icons/ru-RU/rtf_italic.gif"/>
        </Image.Source>
    </Image>
    <Image x:Key="Underline">
        <Image.Source>
            <BitmapImage UriSource="/Resources/Icons/ru-RU/rtf_underline.gif"/>
